<MudDialog>
    <DialogActions>
        <MudButton OnClick="Cancel">Нет</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit">Да</MudButton>
    </DialogActions>
</MudDialog>

@code {

    [CascadingParameter]
    MudDialogInstance MudDialog { get; set; } = null!;

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}